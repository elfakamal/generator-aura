			"templates": [
				"base-collection",
				"base-datasource",
				"base-datastore",
				"base-visualization",
				"partial-select-datasources"
			],
			"models": [
				"d3collection",
				"datastore",
				"visualization",
				"datasource"
			],
			"views": [
				{
					"name": "d3collection",
